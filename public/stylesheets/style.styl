body {background-image:url('../images/grid_noise.png');}

#info

#navi
    text-align center

#goLeft, #zoomOut, #zoomIn, #goRight, #go, #goToFeature, #filter
    cursor pointer

#graphs
  position absolute
  top 180px

#chart, #tree
  float left
  padding 40

.included-row
    color green

.excluded-row
    color tomato

svg
  font 10px sans-serif

circle
  opacity 0.6

rect
  opacity 0.6


div#hjaf
  outline 1px dashed red

.geneLabel
  font 9px sans-serif

.snpTip
  font 7px

.node circle
  fill #fff
  stroke steelblue
  stroke-width 1.5px
  opacity 0.25

.link
  fill none
  stroke #ccc
  stroke-width 1.5px

.ui-dialog-content
  font-family "Helvetica Neue", "Lucida Grande", "Arial"
  font-size 13px

.ui-selecting
  background #b9e0f5

.ui-selected
  background #b9e0f5

.hap
  fill steelblue
  opacity 0.2

.refHap
    fill orange
    opacity 0.2

.strainFrac
    text-color steelblue

.ruleLine
  stroke black
  stroke-opacity 0.1

.message
  font-size 15px
  stroke orangered
.rselect
  color royalblue
