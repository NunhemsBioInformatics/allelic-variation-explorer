import random

haplos = []
for i in range(10):
  s = random.randint(0,50)
  e = random.randint(0,50)
  haplos.append((s,e))

def are_overlaping(a,b):
  if a[0] < b[1] and a[1] > b[0]:
    return True
  else:
    return False

non_overlaps = [];
non_overlaps[0] = [haplos[0]]
for i in range(1, len(haplos)):
  for nov in non_overlaps:
    overlap = 0;
    for hap in nov:
      if are_overlaping(haplos[i], hap):
        overlap = 1;
    if !overlap:
      
